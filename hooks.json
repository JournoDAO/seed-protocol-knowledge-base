[
  {
    "id": "github-webhook",
    "execute-command": "/scripts/deploy.sh",
    "command-working-directory": "/scripts",
    "response-message": "Script executed successfully",
    "trigger-rule": {
      "match": {
        "type": "payload-hash-sha1",
        "secret": "{{GITHUB_WEBHOOK_SECRET}}"
      }
    }
  }
]
